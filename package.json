{
  "name": "ngx-tethys",
  "version": "9.0.0-beta.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run build",
    "build:ng": "rimraf ./built/* && node --max_old_space_size=8096 ./node_modules/@angular/cli/bin/ng build ngx-tethys --prod && cpx \"./src/**/*.scss\" built -v",
    "build:ngc": "rimraf ./built/* ./codegen/* && ngc -p ./src/tsconfig.lib.json",
    "build:ngm": "ngm build -p src --clean && cpx './built/.tmp/**' ./built && rimraf ./built/.tmp",
    "build:theme": "gulp build-theme",
    "build:theme:watch": "npm run build:theme && gulp build-theme:watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg src/package.json",
    "release": "wpm release",
    "release-next": "node ./scripts/release-next.js",
    "pub-only": "cd ./built && npm publish && cd ..",
    "pub": "wpm publish",
    "ng": "ng",
    "start": "npm run build:theme && concurrently \"npm run start:demo\" \"gulp build-theme:watch\"",
    "start:demo": "ng serve demo --port 8888 --disable-host-check",
    "docgeni": "docgeni serve",
    "start:next": "docgeni serve --watch false",
    "build:site": "docgeni build --skip-site",
    "start:site": "npm run build:theme && concurrently \"ng serve site\" \"gulp build-theme:watch\"",
    "build:demo": "npm run build:docs && npm run build:docs:next",
    "build:docs": "ng build demo --prod --extract-css --baseHref=/ngx-tethys/legacy/",
    "build:docs:next": "docgeni build --skip-site && ng build site --prod --extract-css --baseHref=/ngx-tethys/",
    "test": "ng test ngx-tethys",
    "lint": "ng lint",
    "pretty": "pretty-quick --staged",
    "e2e": "ng e2e",
    "serve-demo": "http-server docs -p 8888 -g &"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/atinc/ngx-tethys.git"
  },
  "description": "Angular UI Components",
  "keywords": [
    "Angular",
    "Angular 2+"
  ],
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/forms": "^9.1.12",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@ngx-translate/core": "^12.1.2",
    "ngx-sortablejs": "^3.1.4",
    "bootstrap": "4.5.0",
    "china-division": "^2.1.1",
    "core-js": "^2.5.4",
    "highlight.js": "^9.13.1",
    "date-fns": "^2.6.0",
    "natives": "^1.1.6",
    "ng-packagr": "^9.0.0",
    "ngx-bootstrap": "6.2.0",
    "ngx-highlightjs": "^4.0.2",
    "rxjs": "~6.5.4",
    "sortablejs": "^1.7.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@docgeni/cli": "0.5.6",
    "@docgeni/template": "0.5.6",
    "@tethys/icons": "1.1.12",
    "@types/highlight.js": "^9.12.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/sortablejs": "^1.10.4",
    "@types/webpack": "^4.1.3",
    "@worktile-devkit/commitlint-config": "^1.0.0-beta.18",
    "@worktile-devkit/config": "^1.0.0-beta.18",
    "@worktile-devkit/pm": "^1.0.0-beta.18",
    "@worktile/devkit": "^0.3.3",
    "chalk": "^2.4.2",
    "codelyzer": "^5.1.2",
    "commander": "^2.19.0",
    "concurrently": "^3.6.0",
    "conventional-changelog-cli": "^2.0.29",
    "cpx": "^1.5.0",
    "fs-extra": "^8.0.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-cli": "^2.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "http-server": "^0.11.1",
    "husky": "^2.4.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ngm-cli": "^1.0.4",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "protractor": "~7.0.0",
    "raw-loader": "1.0.0",
    "readline-sync": "^1.4.9",
    "replace": "^1.1.0",
    "semver": "^6.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack": "^4.23.1"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm run build",
      "postchangelog": "node ./scripts/remove-changelog-header.js"
    },
    "skip": {
      "commit": true,
      "tag": true
    }
  }
}
