{
  "name": "ngx-tethys",
  "version": "7.6.16",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:ngm",
    "build:ng-pgr": "rimraf ./built/* ./codegen/* && ng build ngx-tethys && cpx \"./src/**/**/*.scss\" built",
    "build:ng": "ng build ngx-tethys --prod",
    "build:ngc": "rimraf ./built/* ./codegen/* && ngc -p ./src/tsconfig.lib.json",
    "build:ngm": "ngm build -p src --clean",
    "build:theme": "gulp build-theme",
    "build:theme:watch": "npm run build:theme && gulp build-theme:watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg src/package.json",
    "release": "node ./scripts/release.js",
    "release-next": "node ./scripts/release-next.js",
    "pub-only": "cd ./built && npm publish && cd ..",
    "pub": "npm run build && npm run pub-only",
    "ng": "ng",
    "start": "npm run build:theme && concurrently \"ng serve --port 8888\" \"gulp build-theme:watch\"",
    "build:demo": "npm run build:docs",
    "build:docs": "ng build demo --prod --extract-css --baseHref=/ngx-tethys/",
    "test": "ng test ngx-tethys",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve-demo": "http-server docs -p 8888 -g &"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/worktile/ngx-tethys.git"
  },
  "description": "Angular UI Components",
  "keywords": [
    "Angular",
    "Angular 2+"
  ],
  "dependencies": {
    "@angular/animations": "^7.2.6",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.6",
    "@angular/compiler": "^7.2.6",
    "@angular/core": "^7.2.6",
    "@angular/forms": "^7.2.6",
    "@angular/http": "^7.2.6",
    "@angular/platform-browser": "^7.2.6",
    "@angular/platform-browser-dynamic": "^7.2.6",
    "@angular/router": "^7.2.6",
    "@ngx-translate/core": "^10.0.1",
    "angular-sortablejs": "^2.5.2",
    "bootstrap": "^4.3.1",
    "china-division": "^2.1.1",
    "core-js": "^2.5.4",
    "highlight.js": "^9.13.1",
    "natives": "^1.1.6",
    "ng-packagr": "^4.7.1",
    "ngx-bootstrap": "^3.2.0",
    "ngx-highlightjs": "^3.0.0",
    "node-sass": "^4.12.0",
    "rxjs": "^6.4.0",
    "sortablejs": "^1.7.0",
    "tsickle": "^0.34.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.2",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/language-service": "^7.2.6",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "@types/webpack": "^4.1.3",
    "chalk": "^2.4.2",
    "codelyzer": "~4.5.0",
    "commander": "^2.19.0",
    "concurrently": "^3.6.0",
    "conventional-changelog-cli": "^2.0.12",
    "cpx": "^1.5.0",
    "fs-extra": "^8.0.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "http-server": "^0.11.1",
    "husky": "^2.4.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngm-cli": "^1.0.4",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.3.0",
    "readline-sync": "^1.4.9",
    "replace": "^1.1.0",
    "semver": "^6.0.0",
    "standard-version": "^5.0.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.20.0",
    "typescript": "~3.2.4",
    "webpack": "^4.23.1"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm run build",
      "postchangelog": "node ./scripts/remove-changelog-header.js"
    },
    "skip": {
      "commit": true,
      "tag": true
    }
  }
}
