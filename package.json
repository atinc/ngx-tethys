{
  "name": "ngx-tethys",
  "version": "9.0.0-beta.3",
  "license": "MIT",
  "scripts": {
    "start": "docgeni serve",
    "start:site": "docgeni serve",
    "serve": "docgeni serve",
    "watch:theme": "npm run build:theme && gulp build-theme:watch",
    "build:demo": "npm run build:site",
    "build:site": "npm run build:docs && ng build site --prod --extract-css --baseHref=/ngx-tethys/ && npm run build:theme",
    "build:docs": "docgeni build --skip-site",
    "build": "npm run build:ng && npm run build:schematics",
    "build:ng": "rimraf ./built/* && node --max_old_space_size=8096 ./node_modules/@angular/cli/bin/ng build ngx-tethys --prod && npm run build:styles && npm run build:testing",
    "build:styles": "cpx \"./src/**/*.scss\" built && npm run scss-bundle",
    "scss-bundle": "scss-bundle --config ./scss-bundle.config.json && scss-bundle --config ./scss-bundle-basic.config.json",
    "build:testing": "cpx \"./src/core/testing/**/*.ts\" ./built/core/testing",
    "build:theme": "gulp build-theme",
    "release": "wpm release",
    "release-next": "node ./scripts/release-next.js",
    "pub-only": "cd ./built && npm publish && cd ..",
    "pub": "wpm publish",
    "test": "ng test ngx-tethys && npm run test:schematics",
    "test:ngx-tethys": "ng test ngx-tethys",
    "lint": "ng lint ngx-tethys",
    "pretty": "pretty-quick --staged",
    "e2e": "ng e2e",
    "serve-demo": "http-server dist/site -p 8888 -g &",
    "build:schematics": "tsc -p ./schematics && npm run copy:schematics",
    "copy:schematics": "cpx \"./schematics/**/*.{json,template}\" ./built/schematics/ -v",
    "test:schematics": "TS_NODE_PROJECT=./schematics/tsconfig.json node --require ts-node/register/transpile-only ./node_modules/jasmine/bin/jasmine schematics/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/atinc/ngx-tethys.git"
  },
  "description": "Angular UI Components",
  "keywords": [
    "Angular",
    "Angular 2+"
  ],
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/forms": "^9.1.12",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@ngx-translate/core": "^12.1.2",
    "bootstrap": "4.5.0",
    "china-division": "^2.1.1",
    "core-js": "^2.5.4",
    "highlight.js": "^9.13.1",
    "date-fns": "^2.6.0",
    "natives": "^1.1.6",
    "ng-packagr": "^9.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/core": "^9.0.0",
    "@angular-devkit/schematics": "^9.0.0",
    "@schematics/angular": "^9.0.0",
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@docgeni/cli": "0.5.6",
    "@docgeni/template": "0.5.6",
    "@tethys/icons": "1.1.12",
    "@types/highlight.js": "^9.12.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/webpack": "^4.1.3",
    "@worktile-devkit/commitlint-config": "^1.0.0-beta.23",
    "@worktile-devkit/config": "^1.0.0-beta.23",
    "@worktile-devkit/pm": "^1.0.0-beta.23",
    "@worktile/devkit": "^0.3.3",
    "chalk": "^2.4.2",
    "codelyzer": "^5.1.2",
    "commander": "^2.19.0",
    "concurrently": "^3.6.0",
    "conventional-changelog-cli": "^2.0.29",
    "cpx": "^1.5.0",
    "fs-extra": "^8.0.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-cli": "^2.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "http-server": "^0.11.1",
    "husky": "^2.4.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ngm-cli": "^1.0.4",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "protractor": "~7.0.0",
    "raw-loader": "1.0.0",
    "readline-sync": "^1.4.9",
    "replace": "^1.1.0",
    "scss-bundle": "^3.1.2",
    "semver": "^6.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack": "^4.23.1"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
