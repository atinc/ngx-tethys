{
  "compilerOptions": {
    "outDir": "../dist/tethys/schematics",
    "rootDir": "./",
    "target": "ES2020",
    "module": "commonjs",
    "declaration": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "noEmitHelpers": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
